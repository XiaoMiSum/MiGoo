[
  {
    "name": "the test suite name",
    "config": {
      "variables":{
        "user": "admin"
      },
      "request": {
        "url": "http://127.0.0.1:8080/login/login",
        "method": "post",
        "headers": {
          "Content-Type": "application/x-www-form-urlencoded"
        }
      }
    },
    "case": [
      {
        "title": "正确的用户名\\密码登录，返回值包等于期望值",
        "setUp.hook": {
          "pwd": "${getPwd(123456)}"
        },
        "body": {
          "user": "$user",
          "pwd": "$pwd"
        },
        "validate": [
          {
            "check": "$check",
            "expect": 200,
            "types": "=="
          },
          {
            "check": "body",
            "expect": "success",
            "types": "eq"
          }
        ]
      }
    ]
  }
]
