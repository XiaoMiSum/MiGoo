[
  {
    "name": "基础用法",
    "config": {
      "request": {
        "url": "http://migoo.xyz/api/login",
        "method": "post",
        "headers": {
          "Content-Type": "application/json"
        }
      }
    },
    "case": [
      {
        "title": "未注册的用户",
        "body": {
          "userName": "migoo1",
          "password": "123456qq"
        },
        "validate": [
          {"check": "status", "expect": 200, "types": "=="},
          {"check": "$.status", "expect": "411", "types": "eq"},
          {"check": "body.data", "expect": "", "types": "null"}
        ]
      },
      {
        "title": "密码错误",
        "body": {
          "userName": "migoo",
          "password": "123456"
        },
        "validate": [
          {"check": "status", "expect": 200, "types": "=="},
          {"check": "$.status", "expect": 411, "types": "eq"},
          {"check": "body.data", "expect": null, "types": "null"}
        ]
      },
      {
        "title": "参数正确",
        "body": {
          "userName": "migoo",
          "password": "123456qq"
        },
        "validate": [
          {"check": "status", "expect": 200, "types": "=="},
          {"check": "$.status", "expect": "200", "types": "eq"},
          {"check": "body.data.token", "expect": "", "types": "isNotNull"}
        ]
      }
    ]
  }
]
